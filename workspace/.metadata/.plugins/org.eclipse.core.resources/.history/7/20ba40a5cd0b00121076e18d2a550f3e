package ad.model.ExpandableList;

import java.util.ArrayList;  
/**
 * 
 * @author Stefan Arvidsson
 *
 */
public class ExpListGroup {  
	// Initiates the group's instances
     private String id;  						//name
     private ArrayList<ExpListChild> content;   //items
     /**
      * getId - Returns the id of the group.  
      * @return
      */
     public String getId() {  
         return id;  
     }  
     /**
      * setId - Sets the incoming String as the group's new id.
      * @param id
      */
     public void setId(String id) {  
         this.id = id;  
     }  
     /**
      * getContent - 
      * @return
      */
     public ArrayList<ExpListChild> getContent() {  
         return content;  
    }  

     public void setItems(ArrayList<ExpListChild> content) {  
		this.content = content;  
     }          

} 
