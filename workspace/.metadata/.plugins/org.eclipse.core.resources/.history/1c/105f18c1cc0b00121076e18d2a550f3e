package ad.model.ExpandableList;

/**
 * ExpListChild class
 * 
 * Has the state of a child in the expandable list.
 * Instances:
 * -content			The content of the child
 * -label	
 * Methods:
 * -getContent
 * -setContent
 * -getLabel
 * -setLabel
 * @author Stefan Arvidsson
 *
 */
public class ExpListChild {
	// Initiates the child's instances
    private String content;  	//name 
    private String label;  //tag or content

    /**
     * getContent - Returns the content of the child.
     * @return
     */
    public String getContent() {  
        return content;  
    }  
    /**
     * setContent - Sets the incoming String as the child's new content.
     * @param content
     */
    public void setContent(String content) {  
        this.content = content;  
    }  
    /**
     * getLabel - Returns the child's label.
     * @return
     */
   public String getLabel() {  
       return label;  
       
   }  
   /**
    * setLabel - Sets the incoming String as the child's new label.
    * @param label
    */
    public void setTag(String label) {  
        this.label = label;  
   }  
	
}
